
(rules PCB creeper.dsn
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout on)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 150)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 1)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 3.5)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 3.5)
    )
  )
  (rule
    (width 200.0)
    (clearance 200.0)
    (clearance 1000.0 (type smd_to_turn_gap))
    (clearance 50.0 (type smd))
    (clearance 200.0 (type "kicad_default"))
    (clearance 200.0 (type GND,Default))
  )
  (padstack "Via[0-1]_600:300_um"
    (shape
      (circle F.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 600.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_600:300_um" "Via[0-1]_600:300_um" default
  )
  (via 
    "Via[0-1]_600:300_um-kicad_default" "Via[0-1]_600:300_um" "kicad_default"
  )
  (via 
    "Via[0-1]_600:300_um-GND,Default" "Via[0-1]_600:300_um" GND,Default
  )
  (via_rule
    default "Via[0-1]_600:300_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_600:300_um-kicad_default"
  )
  (via_rule
    GND,Default "Via[0-1]_600:300_um-GND,Default"
  )
  (class "kicad_default"
    GND /+3V "Net-(BT1-+)" "Net-(D1-A)" "Net-(D2-A)" "Net-(BZ1-+)" /INT /SCL
    /SDA /LED "Net-(U1-~{RESET}{slash}PB5)" /BUZZER "Net-(U2-CS)" "unconnected-(U2-INT2-Pad11)" "unconnected-(SW1-Pad1)"
    (clearance_class "kicad_default")
    (via_rule "kicad_default")
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class GND,Default
    (clearance_class GND,Default)
    (via_rule GND,Default)
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)